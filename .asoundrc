</home/snoop/.asoundrc.asoundconf>
pcm.usb
{
    type hw
    card CODEC 
}

pcm.!default
{
    type asym
    playback.pcm
    {
        type plug
        slave.pcm "dmix"
    }
    capture.pcm
    {
        type plug
        slave.pcm "usb"
    }
}

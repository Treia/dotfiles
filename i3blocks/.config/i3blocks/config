[music]
label= 
command=$HOME/.config/scripts/playing.py --layout t-A
interval=5

[updates]
command=$HOME/.config/scripts/updates.sh
interval=10000
signal=3
urgent=true

[volume]
label=  
command=~/.config/scripts/volume
interval=1
signal=1
markup=pango
border=#cc241d

[bandwidth]
command=~/.config/scripts/bandwidth
interval=5
border=#d65d0e

[memory]
label= 
command=~/.config/scripts/memory
interval=30
border=#d79921

[disk]
label= 
command=~/.config/scripts/disk
interval=30
border=#b16286

[wireless]
label=   
command=[[ -z "$(iwgetid -r)" ]] || echo "$(iwgetid -r) "
interval=2
border=#98971a

[cpu]
label= 
command=~/.config/scripts/cpu_usage
interval=15
border=#458588

[time]
label=  
command=echo "`date +'%a %d %b'` <b>`date +'%T'`</b> "; [[ -z "${BLOCK_BUTTON}" ]] || gsimplecal &
interval=1
markup=pango
border=#928374
separator_block_width=0
